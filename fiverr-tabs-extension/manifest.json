{
    "manifest_version": 3,
    "name": "Open Fiverr Tabs",
    "description": "Open tabs for Fiverr usernames in batches of 10",
    "version": "1.0",
    "permissions": [
      "tabs",
      "storage",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ]
  }
  